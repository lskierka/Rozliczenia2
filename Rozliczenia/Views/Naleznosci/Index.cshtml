@model IEnumerable<Rozliczenia.Models.NaleznosciViewModel>

@{
    ViewBag.Title = "Naleznosci";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <br />
    <br />
    <br />
    <br />

    <div class="datagrid">
        <table>
            <thead>
                <tr>

                    <th width="80">
                        @Html.DisplayNameFor(model => model.IdKontraktu)
                    </th>
                    <th width="240">
                        @Html.DisplayNameFor(model => model.Usluga)
                    </th>
                    <th width="200">
                        @Html.DisplayNameFor(model => model.OkresDataOd)
                    </th>
                    <th width="200">
                        @Html.DisplayNameFor(model => model.OkresDataDo)
                    </th>
                    <th width="100">
                        @Html.DisplayNameFor(model => model.Kwota)
                    </th>
                    <th width="100">
                        @Html.DisplayNameFor(model => model.KwotaRozliczona)
                    </th>
                    <th width="100">
                        @Html.DisplayNameFor(model => model.DoZaplaty)
                    </th>
                    <th width="100">
                        @Html.DisplayNameFor(model => model.IdPartiiNaleznosci)
                    </th>
                </tr>
            </thead>

            @foreach (var item in Model)
            {

                <tbody>
                    <tr>

                        <td width="80">
                            @Html.DisplayFor(modelItem => item.IdKontraktu)
                        </td>
                        <td width="240">
                            @Html.DisplayFor(modelItem => item.Usluga)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.OkresDataOd)
                        </td>
                        <td width="200">
                            @Html.DisplayFor(modelItem => item.OkresDataDo)
                        </td>
                        <td width="200">
                            @Html.DisplayFor(modelItem => item.Kwota)
                        </td>
                        <td width="100">
                            @Html.DisplayFor(modelItem => item.KwotaRozliczona)
                        </td>
                        <td width="100">
                            @Html.DisplayFor(modelItem => item.DoZaplaty)
                        </td>
                        <td width="100">
                            @Html.DisplayFor(modelItem => item.IdPartiiNaleznosci)
                        </td>
                    </tr>
                </tbody>


            }

        </table>
    </div>
